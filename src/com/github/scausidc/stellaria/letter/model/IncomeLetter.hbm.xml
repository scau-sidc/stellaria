<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.github.scausidc.stellaria.letter.model.IncomeLetter" table="letter_incomeletter">
        <id	name="id" type="long">
            <generator class="identity"/>
        </id>

        <property name="fromUser" type="string" length="32" />

        <property name="content" type="text" length="1024" />

        <property name="msgType" type="string" length="16" />

        <property name="tm" type="timestamp" />
    </class>
</hibernate-mapping>